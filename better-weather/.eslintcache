port { API_KEY, BASE_URL } from \"../constants/index\";\n\nconst initialForm = {\n  search: \"\",\n};\n\nexport default function Search() {\n  const [form, setForm] = useState(initialForm);\n  const [api, setApi] = useState({});\n  const [icon, setIcon] = useState(\"\");\n\n  useEffect(() => {\n    setIcon(getIcon());\n  }, [api]);\n\n  const fetchApi = (search) => {\n    return axios\n      .get(`${BASE_URL}/current.json?key=${API_KEY}&q=${search}`)\n      .then((res) => {\n        return res;\n      })\n      .catch((err) => {\n        return err;\n      });\n  };\n\n  const getIcon = () => {\n    if (!api.hasOwnProperty(\"current\")) return;\n    const icon = api.current.condition.icon;\n    const arr = icon.split(\"\");\n    return \"https:\" + arr.join(\"\");\n  };\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n    fetchApi(form.search)\n      .then((res) => {\n        console.log(\"Fetch Successful ==>> \", res);\n        setApi(res.data);\n      })\n      .catch((err) => {\n        console.log(\"Fetch Failed ==>> \", err);\n      });\n    setForm(initialForm);\n  };\n\n  const handleChange = (e) => {\n    const { name, value } = e.target;\n    setForm({\n      ...form,\n      [name]: value,\n    });\n  };\n\n  return (\n    <div>\n      <form onSubmit={handleSubmit}>\n        <input\n          type=\"text\"\n          name=\"search\"\n          value={form.search}\n          onChange={handleChange}\n        />\n        <button>Search</button>\n      </form>\n      {api.hasOwnProperty(\"location\") && <div>{JSON.stringify(api)}</div>}\n      {api.hasOwnProperty(\"current\") && <img src={icon} alt=\"condition icon\" />}\n    </div>\n  );\n}\n","/home/chrisg/Documents/projects/better_weather/front-end/better-weather/src/App.js",["28"],"/home/chrisg/Documents/projects/better_weather/front-end/better-weather/src/constants/index.js",[],"/home/chrisg/Documents/projects/better_weather/front-end/better-weather/src/utils/getLocation.js",[],"/home/chrisg/D